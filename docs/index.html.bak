    <script>
        // URL API untuk mendapatkan releases dari repository
        const releasesApiUrl = "https://api.github.com/repos/work19-null/apk-vip/releases";
        
        // Tag release spesifik yang ingin ditampilkan
        const targetTag = "apk-vip";
        
        // Variabel global untuk menyimpan semua assets
        let allAssets = [];
        let currentFilter = 'all';
        
        // Fungsi untuk mendapatkan ikon berdasarkan ekstensi file
        function getFileIcon(filename) {
            if (filename.toLowerCase().endsWith('.apk')) {
                return '<i class="fas fa-mobile-alt"></i>';
            } else if (filename.toLowerCase().endsWith('.zip')) {
                return '<i class="fas fa-file-archive"></i>';
            } else {
                return '<i class="fas fa-file"></i>';
            }
        }
        
        // Fungsi untuk mendapatkan warna ikon berdasarkan ekstensi file
        function getIconColor(filename) {
            if (filename.toLowerCase().endsWith('.apk')) {
                return '#58a6ff';
            } else if (filename.toLowerCase().endsWith('.zip')) {
                return '#f78166';
            } else {
                return '#8b949e';
            }
        }
        
        // Fungsi untuk filter file berdasarkan tab
        function filterFiles(filter) {
            currentFilter = filter;
            
            document.querySelectorAll('.file-tab').forEach(tab => {
                tab.classList.remove('active');
            });
            document.getElementById(`tab${filter.charAt(0).toUpperCase() + filter.slice(1)}`).classList.add('active');
            
            renderFileList();
        }
        
        // Fungsi untuk render file list berdasarkan filter
        function renderFileList() {
            const fileListContainer = document.getElementById('fileList');
            
            if (!allAssets || allAssets.length === 0) {
                return;
            }
            
            let filteredAssets = allAssets;
            
            if (currentFilter === 'apk') {
                filteredAssets = allAssets.filter(asset => 
                    asset.name.toLowerCase().endsWith('.apk')
                );
            } else if (currentFilter === 'zip') {
                filteredAssets = allAssets.filter(asset => 
                    asset.name.toLowerCase().endsWith('.zip')
                );
            }
            
            // Hapus semua file item tapi pertahankan release info
            const releaseInfo = fileListContainer.querySelector('.info-box:first-child');
            fileListContainer.innerHTML = '';
            if (releaseInfo) {
                fileListContainer.appendChild(releaseInfo);
            }
            
            // Tambahkan statistik
            const fileCountInfo = document.createElement('div');
            fileCountInfo.className = 'info-box';
            fileCountInfo.style.marginBottom = '20px';
            fileCountInfo.style.backgroundColor = 'rgba(56, 139, 253, 0.05)';
            
            let totalApk = allAssets.filter(a => a.name.toLowerCase().endsWith('.apk')).length;
            let totalZip = allAssets.filter(a => a.name.toLowerCase().endsWith('.zip')).length;
            
            fileCountInfo.innerHTML = `
                <h3><i class="fas fa-chart-bar"></i> Statistik File</h3>
                <p>Total File: <strong>${allAssets.length}</strong> (APK: ${totalApk} file, ZIP: ${totalZip} file)</p>
                <p style="margin-top: 8px;">Menampilkan: <strong>${currentFilter === 'all' ? 'Semua File' : currentFilter.toUpperCase()}</strong> (${filteredAssets.length} file)</p>
            `;
            fileListContainer.appendChild(fileCountInfo);
            
            // Tambahkan file
            filteredAssets.forEach((asset) => {
                const fileItem = document.createElement('div');
                fileItem.className = 'file-item';
                
                const fileSize = (asset.size / (1024*1024)).toFixed(2);
                const fileIcon = getFileIcon(asset.name);
                const iconColor = getIconColor(asset.name);
                
                fileItem.innerHTML = `
                    <div class="file-info">
                        <div class="file-icon" style="color: ${iconColor}">
                            ${fileIcon}
                        </div>
                        <div class="file-details">
                            <h3>${asset.name.replace(/\.(apk|zip)$/, '').replace(/_/g, ' ').replace(/-/g, ' ')}</h3>
                            <p>File ${asset.name.toLowerCase().endsWith('.apk') ? 'APK' : 'ZIP'} dari release <strong>${targetTag}</strong></p>
                            <p style="margin-top: 8px;">
                                <strong>Nama file:</strong> <span class="repo-path">${asset.name}</span> | 
                                <strong>Format:</strong> ${asset.name.split('.').pop().toUpperCase()} | 
                                <strong>Ukuran:</strong> ${fileSize} MB | 
                                <strong>Download:</strong> ${asset.download_count}x
                            </p>
                        </div>
                    </div>
                    <a href="${asset.browser_download_url}" class="download-btn" download>
                        <i class="fas fa-download"></i> Download ${asset.name.split('.').pop().toUpperCase()}
                    </a>
                `;
                
                fileListContainer.appendChild(fileItem);
            });
        }
        
        // Fungsi untuk mendapatkan file dari releases GitHub
        async function fetchFilesFromReleases() {
            const fileListContainer = document.getElementById('fileList');
            
            try {
                const response = await fetch(releasesApiUrl);
                
                if (!response.ok) {
                    throw new Error(`Gagal mengambil data releases: ${response.status}`);
                }
                
                const releases = await response.json();
                
                let targetRelease = releases.find(release => release.tag_name === targetTag);
                
                if (!targetRelease && releases.length > 0) {
                    targetRelease = releases[0];
                }
                
                if (!targetRelease) {
                    throw new Error("Tidak ditemukan releases di repository");
                }
                
                allAssets = targetRelease.assets.filter(asset => 
                    asset.name.toLowerCase().endsWith('.apk') || 
                    asset.name.toLowerCase().endsWith('.zip')
                );
                
                if (allAssets.length === 0) {
                    fileListContainer.innerHTML = `
                        <div class="info-box">
                            <h3><i class="fas fa-exclamation-triangle"></i> Tidak Ditemukan File APK atau ZIP</h3>
                            <p>Tidak ditemukan file APK atau ZIP di release <strong>${targetRelease.tag_name}</strong>.</p>
                            <p style="margin-top: 10px;"><a href="https://github.com/work19-null/apk-vip/releases" class="github-link" target="_blank">Periksa Releases</a></p>
                        </div>
                    `;
                    return;
                }
                
                // Kosongkan container
                fileListContainer.innerHTML = '';
                
                // Tampilkan informasi release
                const releaseInfo = document.createElement('div');
                releaseInfo.className = 'info-box';
                releaseInfo.innerHTML = `
                    <h3><i class="fas fa-tag"></i> Release: ${targetRelease.tag_name}</h3>
                    <p><strong>Dibuat:</strong> ${new Date(targetRelease.created_at).toLocaleDateString('id-ID')}</p>
                    <p style="margin-top: 8px;">${targetRelease.body || 'Tidak ada deskripsi release.'}</p>
                `;
                fileListContainer.appendChild(releaseInfo);
                
                // Render file list
                renderFileList();
                
            } catch (error) {
                console.error("Error fetching releases:", error);
                fileListContainer.innerHTML = `
                    <div class="info-box" style="border-left-color: #f78166;">
                        <h3><i class="fas fa-exclamation-triangle"></i> Gagal Memuat Data</h3>
                        <p>Terjadi kesalahan: <strong>${error.message}</strong></p>
                    </div>
                `;
            }
        }
        
        // Efek download
        document.addEventListener('click', function(e) {
            if (e.target.closest('.download-btn')) {
                const btn = e.target.closest('.download-btn');
                const fileName = btn.parentElement.querySelector('.file-details h3').textContent;
                
                const originalContent = btn.innerHTML;
                btn.innerHTML = '<i class="fas fa-spinner fa-spin"></i> Mengunduh...';
                btn.style.opacity = '0.8';
                
                setTimeout(() => {
                    btn.innerHTML = originalContent;
                    btn.style.opacity = '1';
                    alert(`Download ${fileName} dimulai. Cek folder download Anda.`);
                }, 2000);
            }
        });
        
        // Event listener
        document.addEventListener('DOMContentLoaded', function() {
            fetchFilesFromReleases();
            
            document.getElementById('tabAll').addEventListener('click', function() {
                filterFiles('all');
            });
            document.getElementById('tabApk').addEventListener('click', function() {
                filterFiles('apk');
            });
            document.getElementById('tabZip').addEventListener('click', function() {
                filterFiles('zip');
            });
        });
    </script>
</body>
